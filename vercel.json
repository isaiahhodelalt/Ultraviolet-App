{
  "rewrites": [
    { "source": "/uv/service/:match*", "destination": "/uv/service/:match*" },
    { "source": "/uv/(.*)", "destination": "/uv/$1" },
    { "source": "/(.*)", "destination": "/public/index.html" }
  ],
  "cleanUrls": true,
  "trailingSlash": false,
  "headers": [
    {
      "source": "/uv/(.*)",
      "headers": [
        { "key": "Cache-Control", "value": "public, max-age=31536000, immutable" }
      ]
    }
  ]
}
